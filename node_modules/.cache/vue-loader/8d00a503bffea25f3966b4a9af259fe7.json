\r\n\r\n.porqueAdotar {\r\n  grid-column: 1 / 5;\r\n  font-size: 2.5rem;\r\n  text-align: center;\r\n}\r\n\r\n.quemSomos {\r\n  grid-column: 1 / 5;\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  text-align: justify;\r\n  margin: 0 auto;\r\n  padding: 10px;\r\n}\r\n\r\n.quemSomos div {\r\n  border: 1px solid rgba(0, 0, 0, 0.2);\r\n  font-size: 1rem;\r\n  line-height: 35px;\r\n  padding: 20px;\r\n  margin: 10px;\r\n}\r\n\r\n@media(max-width: 760px) {\r\n  section .bloco {\r\n    display: grid;\r\n    grid-template-columns: 1fr;\r\n  }\r\n\r\n  .bloco button {\r\n    margin-bottom: 25px;\r\n  }\r\n\r\n  .bloco img {\r\n    border-radius: 50%;\r\n  }\r\n\r\n  .quemSomos {\r\n    display: grid;\r\n    grid-template-columns: 1fr;\r\n  }\r\n}\r\n</style>\r\n"]}]}